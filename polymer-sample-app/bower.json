{
  "name": "app",
  "version": "0.5.0",
  "authors": [
    "Vincent Guillou <vin.guillou@gmail.com>"
  ],
  "description": "Simple Web-app built with WebComponents and Polymer. This is intended to be used as a base for your own web-app, as it already implements the following: splash screen, mobile compatible web-app (including iOS) metadata, single-page navigation, routing, responsive design, material design, page transition effects and a page initialisation pattern",
  "main": "index.html",
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "webcomponentsjs": "^0.5",
    "core-animated-pages": "Polymer/core-animated-pages#^0.5",
    "core-drawer-panel": "Polymer/core-drawer-panel#^0.5",
    "core-menu": "Polymer/core-menu#^0.5",
    "core-item": "Polymer/core-item#^0.5",
    "core-icons": "Polymer/core-icons#^0.5",
    "paper-ripple": "Polymer/paper-ripple#^0.5",
    "core-header-panel": "Polymer/core-header-panel#^0.5",
    "core-toolbar": "Polymer/core-toolbar#^0.5",
    "paper-icon-button": "Polymer/paper-icon-button#^0.5",
    "more-routing": "Polymore/more-routing#^0.5",
    "splash-element": "*",
    "activable-mixin": "vguillou/activable-mixin#^0.5"
  }
}
